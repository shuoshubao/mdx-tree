    "mime-types": "^2.1.35",
    "portfinder": "^1.0.32",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "antd": "^5.1.1",
    "esbuild": "^0.16.12",
    "filesize": "^8.0.7",
    "highlight.js": "^11.7.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-task-lists": "^2.1.1",
    "mdx-toc": "^1.1.0",
    "punycode": "^2.1.1",
