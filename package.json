{
  "name": "mdx-tree",
  "version": "0.3.1",
  "description": "Visually view your local markdown file system on the webpage.",
  "repository": {
    "url": "https://github.com/shuoshubao/mdx-tree.git",
    "type": "git"
  },
  "author": "shuoshubao <759979885@qq.com>",
  "bin": {
    "mdx-tree": "index.js"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "keywords": [
    "mdx-tree"
  ],
  "scripts": {
    "start": "esbuild lib/index.js  --outdir=dist --loader:.js=jsx --bundle --watch",
